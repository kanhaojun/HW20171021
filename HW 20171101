import java.util.*;
public class Second {
    public static void main (String[] args){
            
       String test;
       Scanner scanner=new Scanner(System.in);
         test =scanner.next();
       int d=Integer.parseInt(test);
        if(d % 2== 0){
            System.out.print("是偶數");
        }else if(d%2!= 0){
            System.out.print("是奇數");
        }
}
