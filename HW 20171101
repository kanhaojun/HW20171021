public class Second {
    public static void main (String[] argv){
        int a=253;
        int b=134;
        System.out.println("a ="+a);
        System.out.println("b ="+b);
        System.out.println((a>b & a<b));   //和
        System.out.println((a>b | a<b));   //或
        System.out.println((a>b ^ a<b));   //非
        
    }
}
